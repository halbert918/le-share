<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <context id="exp" defaultModelType="flat">
        <!-- RowBounds pagination -->
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin" />

        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <!-- jdbc连接 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/db_rdm" userId="root"
                        password="root" />

        <!-- java model -->
        <javaModelGenerator targetPackage="com.tencent.rdm.exp.entity"
                            targetProject="src/main/java">
            <!-- 是否针对string类型的字段在set的时候进行trim调用 -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- mybatis mapper.xml -->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources" />
        <!-- mybatis mapper -->
        <javaClientGenerator targetPackage="com.tencent.rdm.exp.mapper"
                             targetProject="src/main/java" type="XMLMAPPER" />

        <!-- db table -->
        <table tableName="t_exp_center_main" domainObjectName="ExpCenterMain">
            <!-- override text column -->
            <columnOverride column="title" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="download_style" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="background" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="platform_desc" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="feature" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="feedback_style" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="content" javaType="java.lang.String" jdbcType="VARCHAR" />
        </table>
        <table tableName="t_exp_center" domainObjectName="ExpCenter">
            <columnOverride column="feature" javaType="java.lang.String" jdbcType="VARCHAR" />
        </table>ExpCenterMapper
        <table tableName="t_exp_group_user" domainObjectName="ExpUserGroup"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
            <columnOverride column="comment" javaType="java.lang.String" jdbcType="VARCHAR" />
            <columnOverride column="user_ids" javaType="java.lang.String" jdbcType="VARCHAR" />
        </table>
        <table tableName="t_exp_product_group" domainObjectName="ExpProductGroupRelation"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
        </table>
    </context>
</generatorConfiguration>
